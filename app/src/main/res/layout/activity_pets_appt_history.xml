<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".petsApptHistory">

    <TextView
        android:id="@+id/txt_appt_history_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_history_title"
        android:textColor="@color/hpdkblue"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="@color/hpgold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appt_notes2"
        app:layout_constraintVertical_bias="0.608" />

    <TextView
        android:id="@+id/appt_id2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/appt_petname2"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/appt_date2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.119"
        app:layout_constraintStart_toEndOf="@+id/txt_appt_petname2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.961" />

    <TextView
        android:id="@+id/appt_petname2"
        android:layout_width="155dp"
        android:layout_height="21dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/appt_id2"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/appt_id2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toEndOf="@+id/txt_appt_id2" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="@color/hpgold"
        app:layout_constraintBottom_toTopOf="@+id/appt_petname2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appt_notes"
        app:layout_constraintVertical_bias="0.475" />

    <TextView
        android:id="@+id/txt_appt_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_date"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txt_appt_notes"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/appt_date2"
        android:layout_width="119dp"
        android:layout_height="21dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/appt_date2"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/appt_notes2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toEndOf="@+id/txt_appt_date2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.957" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="@color/hpgold"
        app:layout_constraintBottom_toTopOf="@+id/appt_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.773" />

    <TextView
        android:id="@+id/txt_appt_petname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_petname2"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txt_appt_date2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txt_appt_id2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_id2"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txt_appt_petname2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintVertical_bias="0.619" />

    <TextView
        android:id="@+id/txt_appt_petname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginBottom="28dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_petname"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txt_appt_date"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txt_appt_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_id"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txt_appt_petname"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.881" />

    <TextView
        android:id="@+id/appt_notes"
        android:layout_width="202dp"
        android:layout_height="109dp"
        android:layout_marginTop="276dp"
        android:layout_marginEnd="28dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/appt_notes"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.971"
        app:layout_constraintStart_toEndOf="@+id/txt_appt_notes"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_appt_date2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="116dp"
        android:layout_marginBottom="20dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_date2"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/txt_appt_notes2"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/appt_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/appt_date"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/appt_petname"
        app:layout_constraintStart_toEndOf="@+id/txt_appt_id"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.875" />

    <TextView
        android:id="@+id/txt_appt_notes"
        android:layout_width="50dp"
        android:layout_height="27dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="276dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/txt_appt_notes"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/appt_petname"
        android:layout_width="155dp"
        android:layout_height="21dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/appt_petname"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        app:layout_constraintBottom_toTopOf="@+id/appt_date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.309"
        app:layout_constraintStart_toEndOf="@+id/txt_appt_petname"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.863" />

    <TextView
        android:id="@+id/appt_notes2"
        android:layout_width="202dp"
        android:layout_height="109dp"
        android:layout_marginBottom="60dp"
        android:fontFamily="sans-serif-condensed"
        android:text="@string/appt_notes2"
        android:textColor="#2E8BB3"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toEndOf="@+id/txt_appt_notes2" />

    <TextView
        android:id="@+id/appt_date"
        android:layout_width="119dp"
        android:layout_height="21dp"
